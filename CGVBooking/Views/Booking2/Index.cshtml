@model IEnumerable<CGVBooking.Models.TicketBooking>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Index</h2>

<p>
    @Html.ActionLink("Booking", "Booking")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Chair.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ShowTimes.ShowTimesID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Chair.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ShowTimes.ShowTimesID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.Name)
            </td>
           
        </tr>
    }

</table>


